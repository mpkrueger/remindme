<h1>Login</h1>

<%= form_tag '/login' do %>
  <div class:"form-group">
    Phone: <%= telephone_field :phone_number, '', class: 'form-control' %>
  </div>

  <div class: "form-group">
    Password: <%= password_field_tag :password, '', class: 'form-control' %>
  </div>
    
  <%= submit_tag "Submit" %>


<% end %>